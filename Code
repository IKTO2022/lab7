CREATE TABLE transport
(
  id INTEGER NOT NULL,
  brand VARCHAR(50),
  body_type VARCHAR(20),
  engine_power INTEGER,
  max_speed INTEGER,
  seats INTEGER,
  PRIMARY KEY (id)
);
INSERT INTO transport(id, brand, body_type, engine_power, max_speed, seats) VALUES (1, 'Toyota', 'купе', 200, 210, 2);
INSERT INTO transport(id, brand, body_type, engine_power, max_speed, seats) VALUES (2, 'Nissan', 'универсал', 170, 180, 6);
INSERT INTO transport(id, brand, body_type, engine_power, max_speed, seats) VALUES (3, 'Toyota', 'минивен', 250, 230, 7);
