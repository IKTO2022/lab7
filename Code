INSERT INTO transport(id, brand, body_type, engine_power, max_speed, seats) VALUES (5, 'Toyota', 'хэтчбэк', 150, 160, 5);
INSERT INTO transport(id, brand, body_type, engine_power, max_speed, seats) VALUES (6, 'Nissan', 'седан', 230, 210, 4);


